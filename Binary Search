#include <iostream>

using namespace std;
int search(int a[],int x,int n){
    
    for(int i=0;i<n;i++)
        
            if (a[i]==x)
            return i;
         
            
        
    return -1;
}
int main()
{
    int array[5]={5,8,9,78,15};
    int n= sizeof(array)/sizeof(array[0]);
    int x; 
    cin>>x;
   int y= search(array,x,n);
   if(y==-1){
       cout<<"Element not present"<<endl;}
       else
       {
           cout<<"Element is present at index "<<y;
       }
   
    
    return 0;
}
